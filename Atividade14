numero = int(input("Digite um número entre 1 e 7: "))
def obter_dia_da_semana(numero):
    dia = None
    if numero == 1:
        dia = "Domingo"
    elif numero == 2:
        dia = "Segunda-feira"
    elif numero == 3:
        dia = "Terça-feira"
    elif numero == 4:
        dia = "Quarta-feira"
    elif numero == 5:
        dia = "Quinta-feira"
    elif numero == 6:
        dia = "Sexta-feira"
    elif numero == 7:
        dia = "Sábado"
    else:
        dia = "Número inválido"
    return dia

dia_da_semana = obter_dia_da_semana(numero)

print(f"O dia da semana correspondente é {dia_da_semana}")
