import math 

numero = float(input("Dígite um número: "))

if numero >=0:
    quadrado = math.pow(numero, 2)
    raiz_quadrada = math.sqrt(numero)
    print(f"O número {numero} ao quadrado corresponde á {quadrado}, e sua raíz quadrada é dado por {raiz_quadrada}")
else: 
    print(f"Número inválido. Por favor, ensira um número válido.")